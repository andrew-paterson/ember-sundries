<EmberSundries::ModalDialogWrapper
  @activeModal={{@activeModal}}
  @title={{@title}}
>
  <EmberSundries::ConfirmCancelModalContent
    @message={{@message}}
    @closeModal={{@closeModal}}
    @colorScheme={{@colorScheme}}
    @confirmAction={{@confirmAction}}
    @modalName={{@modalName}}
  />
</EmberSundries::ModalDialogWrapper>